{
  "name": "inca-app",
  "description": "Lab Agora web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "node -r dotenv/config ./prisma/seed.js",
    "dev": "node -r dotenv/config ./scripts/dev/start.js",
    "dev:db:restore": "node -r dotenv/config ./scripts/dev/restoreFromProdBackup.js",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.dev.yml up -d db",
    "dev:migrate": "prisma migrate dev",
    "prepare": "husky install && node -r dotenv/config ./scripts/dev/setup.js",
    "prestart": "yarn db:migrate",
    "start": "next start",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint .",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@fontsource/poppins": "4.5.0",
    "@prisma/client": "3.2.1",
    "@singularity/core": "2.0.0-beta.17",
    "bcryptjs": "2.4.3",
    "better-prop-types": "1.0.0",
    "bhala": "1.0.2",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "fs-extra": "10.0.1",
    "jose-browser-runtime": "3.17.0",
    "jsonwebtoken": "8.5.1",
    "ky": "0.30.0",
    "lodash.debounce": "4.0.8",
    "next": "12.1.0",
    "node-cache": "5.1.2",
    "pem-jwk": "2.0.0",
    "prisma": "3.12.0",
    "query-string": "7.0.1",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-router-dom": "6.2.2",
    "react-spinners": "0.11.0",
    "styled-components": "5.3.1",
    "swr": "1.0.1",
    "unique-string": "3.0.0",
    "yup": "0.32.11",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@ivangabriele/commitlint-config": "1.0.2",
    "@ivangabriele/eslint-config-react": "2.1.4",
    "@ivangabriele/prettier-config": "2.0.2",
    "@types/jest": "27.4.1",
    "babel-jest": "27.2.4",
    "babel-plugin-styled-components": "2.0.7",
    "eslint-plugin-jest": "24.4.0",
    "glob": "7.2.0",
    "husky": "7.0.4",
    "jest": "27.2.0",
    "keypair": "1.0.4",
    "shelljs": "0.8.5"
  },
  "prettier": "@ivangabriele/prettier-config"
}
