{
  "name": "inca-app",
  "description": "Lab Agora web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "prisma migrate dev --name init",
    "db:seed": "node -r dotenv/config ./prisma/seed.js",
    "dev": "next dev",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.dev.yml up -d db",
    "prepare": "husky install && node -r dotenv/config ./scripts/dev/setup.js",
    "start": "prisma migrate deploy && next start",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint .",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@fontsource/poppins": "4.5.0",
    "@ivangabriele/singularity": "0.3.2",
    "@prisma/client": "3.2.1",
    "bcryptjs": "2.4.3",
    "bhala": "1.0.0-alpha.1",
    "body-parser": "1.19.0",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "fs-extra": "10.0.0",
    "jose-browser-runtime": "3.17.0",
    "jsonwebtoken": "8.5.1",
    "ky": "0.28.5",
    "mongoose": "6.0.6",
    "next": "11.1.2",
    "pem-jwk": "2.0.0",
    "prisma": "3.2.1",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "styled-components": "5.3.1",
    "swr": "1.0.1",
    "unique-string": "3.0.0",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@ivangabriele/eslint-config-react": "1.5.0",
    "@ivangabriele/prettier-config": "1.1.0",
    "@types/jest": "27.0.2",
    "babel-jest": "27.2.4",
    "babel-plugin-styled-components": "1.13.2",
    "eslint-plugin-jest": "24.4.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "keypair": "1.0.3"
  },
  "prettier": "@ivangabriele/prettier-config"
}
