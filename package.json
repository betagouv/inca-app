{
  "name": "inca-app",
  "description": "Lab Agora web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "16",
    "npm": "8",
    "yarn": "1"
  },
  "scripts": {
    "build": "next build",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "node -r dotenv/config ./prisma/seed.js",
    "dev": "node -r dotenv/config ./scripts/dev/start.js",
    "dev:db:restore": "node -r dotenv/config ./scripts/dev/restoreFromProdBackup.js",
    "dev:docker": "docker compose -f ./docker-compose.dev.yml down && docker compose -f ./docker-compose.dev.yml up -d db",
    "dev:migrate": "prisma migrate dev",
    "dev:setup": "sh ./scripts/dev/setup.sh",
    "prepare": "husky install",
    "prestart": "yarn db:migrate",
    "start": "next start",
    "test": "yarn test:lint && yarn test:type && yarn test:unit",
    "test:lint": "eslint .",
    "test:type": "tsc -p ./tsconfig.json",
    "test:unit": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config=./config/jest.config.js --detectOpenHandles",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "@fontsource/poppins": "4.5.9",
    "@js-temporal/polyfill": "0.4.1",
    "@prisma/client": "4.1.0",
    "@reduxjs/toolkit": "1.8.3",
    "@singularity/core": "4.4.0",
    "axios": "0.27.2",
    "bcryptjs": "2.4.3",
    "better-prop-types": "1.1.1",
    "bhala": "3.0.4",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "ky": "0.30.0",
    "lodash.debounce": "4.0.8",
    "nexauth": "2.1.1",
    "next": "12.2.5",
    "node-cache": "5.1.2",
    "prisma": "4.1.0",
    "query-string": "7.1.1",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "2.0.10",
    "react-icons": "4.4.0",
    "react-redux": "8.0.2",
    "react-spinners": "0.13.4",
    "react-toastify": "8.1.0",
    "styled-components": "5.3.5",
    "superjson": "1.9.1",
    "unique-string": "3.0.0",
    "yup": "0.32.11",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-typescript-next": "4.1.1",
    "@ivangabriele/prettier-config": "3.1.0",
    "@ivangabriele/tsconfig-next": "1.2.2",
    "@jest/globals": "28.1.2",
    "@swc/core": "1.2.218",
    "@swc/jest": "0.2.21",
    "@types/jest": "28.1.4",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.0.3",
    "@types/ramda": "0.28.15",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "esm-path": "1.0.1",
    "glob": "8.0.3",
    "husky": "8.0.0",
    "jest": "27.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.5.1",
    "shelljs": "0.8.5",
    "type-fest": "2.16.0",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{cjs,js,jsx,ts,tsx}": "eslint --ext cjs,js,jsx,ts,tsx"
  },
  "prettier": "@ivangabriele/prettier-config",
  "packageManager": "yarn@1.22.19"
}
